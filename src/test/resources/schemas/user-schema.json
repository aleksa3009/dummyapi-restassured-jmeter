{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User",
  "type": "object",
  "required": ["id", "firstName", "lastName"],
  "properties": {
    "id": {
      "type": "string"
    },
    "title": {
      "type": ["string", "null"]
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "picture": {
      "type": ["string", "null"]
    }
  },
  "additionalProperties": false
}